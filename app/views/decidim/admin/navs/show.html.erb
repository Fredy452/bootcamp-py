<div class="container">
  <div class="process-title">
    <div class="process-title-content">
      <a href="admin/navs">Navbars</a>
    </div>
  </div>
  <div class="process-content">
    <div class="card with-overflow">
      <div class="card-divider">
        <h2 class="card-title">Navbar
          <%= link_to 'Editar', edit_nav_path(@nav), class:"button tiny button--title" %>
          <%= link_to 'Atras', navs_path, class:"button tiny button--title" %>
        </h2>
      </div>
      <div class="card-section">
        <p id="notice"><%= notice %></p>

        <p>
          <strong>Laber:</strong>
          <%= @nav.laber %>
        </p>

        <p>
          <strong>Url:</strong>
          <%= @nav.url %>
        </p>

      </div>
    </div>
  </div>
</div>
